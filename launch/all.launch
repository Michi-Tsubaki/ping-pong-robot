<?xml version="1.0"?>
<launch>
    <!-- Launch ping-pong minimal -->
    <include file="$(find ping-pong)/launch/minimal.launch" />

    <!-- Launch RealSense camera -->
    <include file="$(find realsense2_camera)/launch/rs_rgbd.launch" />

    <!-- Launch ball detection -->
    <include file="$(find ping-pong)/launch/2d-detect.launch" />

    <!-- Launch sound play node -->
    <include file="$(find sound_play)/soundplay_node.launch" />
</launch>
